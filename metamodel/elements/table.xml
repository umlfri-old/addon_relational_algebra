<?xml version="1.0" encoding="utf-8"?>
<ElementType id="Table" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/table.png" />
    <Domain id="table" identity="name" />
    <Connections>
        <Item value="Relationship" allowrecursive="1"/>
    </Connections>
    <Appearance>
    <HBox>
        <Padding top="10">
			<Align align="center center">
				<Svg width="40" height="60">
					<path d="M 0,10 M 0,0 C 0,-10 40,-10 40,0 C 40,10 0,10 0,0 M 0,0 L 0,40 C 0,60 40,60 40,40 L 40,0" style="fill:none;stroke:#000000;stroke-width:2;stroke-miterlimit:4;stroke-dashoffset:0;stroke-opacity:1" />
				</Svg>
            </Align>
        </Padding>
		
		<Align align="center center">
		<VBox>
			<TextBox text="#self.table_name"></TextBox>					
			</VBox>
		</Align>
</HBox>

    </Appearance>
</ElementType>