<?xml version="1.0" encoding="utf-8"?>
<ElementType id="Table" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/table.png" />
    <Domain id="table" identity="name" />
    <Connections>
        <Item value="Relationship" allowrecursive="1"/>
    </Connections>
    <Appearance>
        <Sizer height="80" width="40">
        <HBox>
            <Sizer height="20" width="40">
            <Ellipse border="black">
            </Ellipse>
            </Sizer>
            <Svg width="40" height="20">
                <path d="M 0,-10 L 0,30 C 0,50 40,50 40,30 L 40,-10" style="fill:none;stroke:#000000;stroke-width:2;stroke-miterlimit:4;stroke-dashoffset:0;stroke-opacity:1" />
            </Svg>
            <Padding left="5">
            <TextBox text="#self.name"></TextBox>
            </Padding>
        </HBox>
       
        </Sizer>
    </Appearance>
</ElementType>