<?xml version="1.0" ?>
<AddOn xmlns="http://umlfri.org/xmlschema/addon.xsd">
    <Identity uri="urn:umlfri.org:metamodel:DRAmodel" />

    <FriendlyName name="DRAmodel" version="0.0.1" />
    <Author>
        <Name name="Michal Belas" />
        <Homepage url="http://umlfri.org" />
        
        <CommonLicense name="GPL-3" />
    </Author>
    
    <Icon path="icons/DRA.png" />
    <Description>
        Diagram of relation algebra (DRA) is an abstract and conceptual representation of data.
    </Description>
    <Dependencies>
           <AddOn uri="urn:umlfri.org:starters:python" />
       </Dependencies>
    <Composite>
        <Metamodel>
            <Path path="metamodel" />
            <Template path="templates/empty.fritx" name="Empty DRA Diagram" />
        </Metamodel>
        <Plugin>
            <Code os="all" language="pythonNew" path="plugin" />
        </Plugin>
    </Composite>

</AddOn>
